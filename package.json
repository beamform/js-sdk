{
  "name": "@beamformio/js-sdk",
  "description": "TypeScript SDK for Beamform API",
  "version": "0.1.2",
  "license": "MIT",
  "homepage": "https://github.com/beamform/js-sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/beamform/js-sdk.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm generate:types",
    "build": "vite build",
    "prebuild:analyze": "pnpm generate:types",
    "build:analyze": "vite build --config vite.config.ts --mode analyze",
    "predev": "pnpm generate:types",
    "dev": "vite build --watch",
    "generate:types": "openapi-typescript ./openapi.json -o ./src/generated/schema.d.ts",
    "lint": "biome check",
    "format": "biome check --write",
    "pretest": "pnpm generate:types",
    "test": "vitest run",
    "pretest:watch": "pnpm generate:types",
    "test:watch": "vitest",
    "pretypecheck": "pnpm generate:types",
    "typecheck": "tsc --noEmit",
    "quality": "pnpm typecheck && pnpm lint"
  },
  "keywords": [
    "beamform",
    "sdk",
    "typescript",
    "api"
  ],
  "author": "Beamform",
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "openapi-typescript": "^7.9.1",
    "rollup-plugin-webpack-stats": "^2.1.4",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite-plugin-dts": "^4.5.4",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "packageManager": "pnpm@10.14.0"
}
